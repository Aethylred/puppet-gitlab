    { "name": "puppetlabs/postgresql",  "version_requirement": ">=3.3.0 <4.0.0" },
    { "name": "Aethylred/git",          "version_requirement": ">=0.1.0 <1.0.0" },
    { "name": "thomasvandoren/redis",   "version_requirement": ">=0.10.0 <1.0.0" }
  ] 
}